{% extends 'base.html.twig' %}

{% block body %}
    <h1>Ulimg</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ulImg.id }}</td>
            </tr>
            <tr>
                <th>File</th>
                <td>{{ ulImg.file }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ ulImg.name }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('ulimg_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('ulimg_edit', { 'id': ulImg.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
